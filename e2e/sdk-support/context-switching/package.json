{
  "name": "@pioneer-platform/support-e2e-context-switching",
  "version": "8.1.23",
  "scripts": {
    "yarn": "yarn",
    "start": "node lib/index.js",
    "build": "tsc -p .",
    "dev": "rm -Rf ~/.pioneer && npm run build && npm run start",
    "test": "rm -Rf ~/.pioneer && jest --silent=true --verbose --detectOpenHandles --forceExit",
    "docker:build": "docker build --build-arg NPM_TOKEN=$NPM_TOKEN -t pioneer/eth-e2e-sdk:latest .",
    "docker:run": "docker run --env-file=../../.env pioneer/eth-e2e-sdk:latest",
    "docker:tag": "docker tag pioneer/eth-e2e-sdk:latest registry.digitalocean.com/pioneer/pioneer/eth-e2e-sdk:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/eth-e2e-sdk:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "dependencies": {
    "@pioneer-platform/loggerdog": "^8.1.9",
    "@pioneer-platform/midgard-client": "^8.1.20",
    "@pioneer-platform/pioneer-app": "^8.1.25",
    "@pioneer-platform/pioneer-app-e2e": "^8.1.26",
    "@pioneer-platform/pioneer-sdk": "^8.1.27",
    "assert": "^2.0.0",
    "dotenv": "^6.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "typescript": "^4.2.4",
    "wait-promise": "^0.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "gitHead": "61817016c96d9a1fa3d741d440776ad2dbce8422"
}
